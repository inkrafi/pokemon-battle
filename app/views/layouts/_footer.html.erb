    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
    
    <!-- Custom JavaScript -->
    <script>
      document.addEventListener('DOMContentLoaded', function () {
        var pokedexCards = document.querySelectorAll('.pokedex-card');
        var skillCheckboxes = document.querySelectorAll('.skill-checkbox');

        pokedexCards.forEach(function (card) {
          card.addEventListener('click', function () {
            var selectedTypeId = card.getAttribute('data-type-id');

            // Sembunyikan semua checkbox skill menggunakan kelas Bootstrap d-none
            skillCheckboxes.forEach(function (checkbox) {
              checkbox.classList.add('d-none');
            });

            // Tampilkan hanya checkbox skill yang sesuai dengan tipe yang dipilih
            var selectedTypeSkillCheckboxes = document.querySelectorAll('.skill-checkbox[data-type-id="' + selectedTypeId + '"]');
            selectedTypeSkillCheckboxes.forEach(function (checkbox) {
              checkbox.classList.remove('d-none');
            });
          });
        });
      });
    </script>
  </body>
</html>